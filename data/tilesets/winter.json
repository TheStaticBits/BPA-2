{
    "name": "Winter",
    "baseTile": " ",
    "music": "res/sounds/music/woahExtendedModified.wav",

    "tiles": {
        " ": { "move": "none",    "path": "res/tiles/winter/tile.png"      },
        "-": { "move": "forward", "path": "res/tiles/winter/path.png"      },
        "l": { "move": "left",    "path": "res/tiles/winter/turnLeft.png"  },
        "r": { "move": "right",   "path": "res/tiles/winter/turnRight.png" },
        "+": { "move": "forward", "path": "res/tiles/winter/cross.png"     },
        "T": { "move": "left",    "path": "res/tiles/winter/tPath.png"     }
    },
    "rotated": {
        "|": { "tile": "-", "degrees":  90 },

        "R": { "tile": "r", "degrees": -90 },
        "k": { "tile": "r", "degrees": 180 },
        "K": { "tile": "r", "degrees":  90 },
        
        "L": { "tile": "l", "degrees":  90 },
        "i": { "tile": "l", "degrees": 180 },
        "I": { "tile": "l", "degrees": -90 },

        "t": { "tile": "T", "degrees": 180, "move": "right" }
    },
    "unmovable": {
        "e": { "tile": " " }
    },
    "deco": {
        "s": {
            "tile": " ",
            "placable": false, "moveWithTile": true, "offset": [ 20, 15 ],
            "animation": { "path": "res/tiles/winter/deco/snowman.png", "frames": 2, "delay": 0.7 }
        },
        "1": {
            "tile": " ",
            "placable": false, "moveWithTile": true, "offset": [ 20, 20 ],
            "animation": { "path": "res/tiles/winter/deco/tree1.png", "frames": 2, "delay": 2.5 }
        }
    }
}